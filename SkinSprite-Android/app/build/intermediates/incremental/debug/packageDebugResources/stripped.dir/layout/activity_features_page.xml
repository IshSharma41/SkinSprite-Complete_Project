<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/features_page_bg"
    tools:context=".FeaturesPageActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:background="@drawable/app_logo_inapp"
        android:gravity="center"
        android:text="@string/app_name"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="95dp"
        android:layout_height="149dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pixie" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="131dp"
        android:layout_height="89dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/user_profile_icon" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="231dp"
        android:layout_height="21dp"
        android:gravity="center"
        android:text="Hi User"
        android:textColor="@color/indigo"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="231dp"
        android:layout_height="28dp"
        android:gravity="center"
        android:text="Good Morning"
        android:textColor="@color/yellow"
        android:textSize="20sp"
        android:textStyle="italic|bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="502dp"
        android:background="@drawable/features_page_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="1.0">

        <TableRow
            android:id="@+id/tableRow"
            android:layout_width="377dp"
            android:layout_height="108dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.47"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.041">

            <ImageButton
                android:id="@+id/btn1"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_1" />

            <ImageButton
                android:id="@+id/btn2"
                android:layout_width="128dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_2" />

            <ImageButton
                android:id="@+id/btn3"
                android:layout_width="124dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_3" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="375dp"
            android:layout_height="110dp"
            android:layout_marginStart="2dp"
            android:layout_marginBottom="266dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableRow">

            <ImageButton
                android:id="@+id/btn4"
                android:layout_width="123dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_4" />

            <ImageButton
                android:id="@+id/btn5"
                android:layout_width="128dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_5" />

            <ImageButton
                android:id="@+id/btn6"
                android:layout_width="123dp"
                android:layout_height="match_parent"
                android:background="@drawable/bg_login_signup_button"
                android:clickable="true"
                app:srcCompat="@drawable/btn_6" />

        </TableRow>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="349dp"
            android:layout_height="163dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/health_tip_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableRow2"
            app:layout_constraintVertical_bias="0.0">

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="159dp"
                android:layout_height="139dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.983"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.236"
                app:srcCompat="@drawable/green_banner" />

            <TextView
                android:id="@+id/TipHeading"
                android:layout_width="165dp"
                android:layout_height="28dp"
                android:gravity="center"
                android:text="@string/HealthTip"
                android:textColor="@color/deep_pink"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.008" />


            <TextView
                android:id="@+id/Tip"
                android:layout_width="162dp"
                android:layout_height="122dp"
                android:text="@string/Tip"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.085"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TipHeading"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="409dp"
            android:layout_height="99dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">


            <ImageButton
                android:id="@+id/logoutBtn"
                android:layout_width="95dp"
                android:layout_height="97dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.949"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/logout_btn" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingActionButtonChatBot"
                android:layout_width="141dp"
                android:layout_height="66dp"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/logoutBtn"
                app:layout_constraintHorizontal_bias="0.068"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.484"
                app:srcCompat="@drawable/icon_bot" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>